<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hotelprogram.ChangeRoomController">
   <children>
      <ListView fx:id="roomList" prefHeight="200.0" prefWidth="600.0" />
      <TextField fx:id="newRoom" layoutX="300.0" layoutY="242.0" />
      <Text layoutX="182.0" layoutY="259.0" strokeType="OUTSIDE" strokeWidth="0.0" text="New Room:" />
      <Button fx:id="confirmBtn" layoutX="213.0" layoutY="314.0" mnemonicParsing="false" onAction="#onClickConfirmRoomChange" text="Confirm Room Change" />
      <Button fx:id="backBtn" layoutX="39.0" layoutY="339.0" mnemonicParsing="false" onAction="#onClickCloseWindow" text="Back" />
   </children>
</AnchorPane>
